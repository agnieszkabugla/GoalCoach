    "@babel/preset-es2015": "^7.0.0-beta.47",
    "@babel/preset-react": "^7.0.0-beta.47",
    "babel-loader": "^8.0.0-beta.0",
    "babel-preset-latest": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "cheerio": "^1.0.0-rc.2",
    "colors": "^1.2.5",
    "copy-webpack-plugin": "^4.5.1",
